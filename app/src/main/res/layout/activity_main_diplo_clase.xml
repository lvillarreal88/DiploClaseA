<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.donquijote.app.diploclasea.MainActivityDiploClaseA"
    android:layout_margin="20dp">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/btn_dos"
        android:textColor="@color/colorWhite"
        android:background="@drawable/diplodrawable"
        android:padding="20dp"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/main_floatingbtn"
        android:layout_width="wrap_content"
        android:src="@drawable/ic_touch_app_black_24dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:onClick="onClick"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/main_btn"
        android:layout_width="wrap_content"
        android:src="@drawable/ic_format_list_numbered_black_24dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:onClick="onClickSpinner"/>


</LinearLayout>
